.icon {
  width: 54rpx;
  height: 54rpx;
  margin: 0 0 10rpx 0;
}

.tabBar {
  width: 100%;
  position: fixed;
  bottom: 0;
  background: #fff;
  font-size: 20rpx;
  color: #8a8a8a;
  height: 163rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  border-top: 1rpx solid #f3f3f3;
}

.tabBar-item {
  display: flex;
  flex: 1;
  text-align: center;
  overflow: hidden;
  font-size: 25rpx;
  color: #000;
  justify-content: center;
}

.tabBar .tabBar-item:nth-of-type(2) .down_href {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: -20rpx;
  width: 156rpx;
  height: 156rpx;
  z-index: 10000;
}

.tabBar .tabBar-item:nth-of-type(2) image {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

/*当前字体颜色*/

.tabBartext {
  color: #ffae0b;
}

